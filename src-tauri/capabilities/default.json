{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "opener:allow-open-path",
      "allow": [
        {
          "path": "**"
        }
      ]
    },
    "core:window:allow-close",
    "core:window:allow-toggle-maximize",
    "core:window:allow-minimize",
    "core:window:allow-start-dragging",
    "core:window:allow-set-fullscreen",
    "drpc:allow-clear-activity",
    "deep-link:default",
    "deep-link:allow-register",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://weebcentral.com/*"
        },
        {
          "url": "https://*.weebcentral.com/*"
        },
        {
          "url": "https://temp.compsci88.com/*"
        },
        {
          "url": "https://*.compsci88.com/*"
        },
        {
          "url": "https://planeptune.us/*"
        },
        {
          "url": "https://*.planeptune.us/*"
        },
        {
          "url": "https://hianime.to/*"
        },
        {
          "url": "https://*.hianime.to/*"
        },
        {
          "url": "https://animepahe.ru/*"
        },
        {
          "url": "https://*.animepahe.ru/*"
        },
        {
          "url": "https://animepahe.org/*"
        },
        {
          "url": "https://*.animepahe.org/*"
        },
        {
          "url": "https://animepahe.com/*"
        },
        {
          "url": "https://*.animepahe.com/*"
        },
        {
          "url": "https://kaido.to/*"
        },
        {
          "url": "https://*.kaido.to/*"
        },
        {
          "url": "https://animekai.to/*"
        },
        {
          "url": "https://*.animekai.to/*"
        },
        {
          "url": "https://gogoanime.hu/*"
        },
        {
          "url": "https://*.gogoanime.hu/*"
        },
        {
          "url": "https://gogoanime.gg/*"
        },
        {
          "url": "https://*.gogoanime.gg/*"
        },
        {
          "url": "https://gogoanime3.co/*"
        },
        {
          "url": "https://*.gogoanime3.co/*"
        },
        {
          "url": "https://graphql.anilist.co/*"
        },
        {
          "url": "https://anilist.co/*"
        },
        {
          "url": "https://*.anilist.co/*"
        },
        {
          "url": "https://megacloud.tv/*"
        },
        {
          "url": "https://*.megacloud.tv/*"
        },
        {
          "url": "https://rapid-cloud.co/*"
        },
        {
          "url": "https://*.rapid-cloud.co/*"
        },
        {
          "url": "http://*"
        },
        {
          "url": "https://*"
        }
      ],
      "deny": []
    },
    "dialog:default",
    "shell:allow-open",
    "process:default",
    "drpc:default",
    "notification:default",
    "fs:default",
    {
      "identifier": "fs:allow-read",
      "allow": [
        {
          "path": "$HOME/**"
        },
        {
          "path": "$DOWNLOAD/**"
        },
        {
          "path": "$DOCUMENT/**"
        },
        {
          "path": "$DESKTOP/**"
        },
        {
          "path": "C:/**"
        },
        {
          "path": "D:/**"
        },
        {
          "path": "E:/**"
        },
        {
          "path": "F:/**"
        },
        {
          "path": "**"
        }
      ]
    }
  ]
}